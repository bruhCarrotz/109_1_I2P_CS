#include <stdio.h>
int main() { 
	char c;
	double a, b;
	scanf("%lf%c%lf", &a, &c, &b);
	if(c == '+') {
		printf("%.3lf\n", a + b);
	} else if(c == '-') {
		printf("%.3lf\n", a - b);
	} else if(c == '*') {
		printf("%.3lf\n", a * b);
	} else if(c == '/') {
		if(b == 0) printf("Error\n");
		else printf("%.3lf\n", a / b);
	}
	return 0;
}
